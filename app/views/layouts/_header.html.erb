<header id="main_header" class="cf">
    <div class="wrapper_wide">
      <%= link_to "Pet Movies", users_path, id: "logo" %>
        <nav>
          <ul>
            <% unless user_signed_in? %>
            <li><%= link_to "All we here", users_path %></li>
            <li><%= link_to "Sign up", new_user_registration_path %></li>
            <li><%= link_to "Log In", new_user_session_path %></li>
            <% else %>
            <li><%= link_to "All we here", users_path %></li>
            <li><%= link_to "Movie manage", authenticated_root_path %></li>
            <li><%= link_to "Logged in as: #{current_user.name}", user_path(current_user) %></li>
            <li><%= link_to "Log Out", destroy_user_session_path, method: :delete %></li>
            <% end %>
        </ul>
      </nav>
    </div>
  </header>
